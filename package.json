{
	"name": "web-store-container",
	"version": "1.0.0",
	"description": "A WP5 MF Base Container",
	"main": "index.jsx",
	"browserslist": [
		"> 0.25%, not dead"
	],
	"scripts": {
		"start": "set BUILD_ENV=developent && webpack-dev-server --config ./webpack/webpack.config.js",
		"build": "webpack --config webpack/webpack.config.js",
		"build-dev": "BUILD_ENV=development npm run build",
		"build-feature": "BUILD_ENV=feature npm run build",
		"build-beta": "BUILD_ENV=beta npm run build",
		"build-prod": "BUILD_ENV=production npm run build",
		"build-client": "BUILD_ENV=client npm run build",
		"analyze": "npm run build-prod && source-map-explorer 'dist/*.js'",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
		"lint-fix": "eslint --ignore-path .gitignore --fix --ext .js,.ts,.tsx .",
		"prettier": "prettier --check src/",
		"format": "prettier --write src/",
		"prepare": "node --experimental-json-modules ./webpack/prepare.mjs"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/eslint-parser": "^7.16.3",
